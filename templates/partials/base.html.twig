<!DOCTYPE html>
<html lang="en">
<head>
    {% block head %}
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        {% if page.header.quick_summary %}
            <meta name="description" content="{{ page.header.quick_summary }}">
        {% else %}
            <meta name="description" content="{{ site.metadata.description }}">
        {% endif %}
        {% if header.robots %}
            <meta name="robots" content="{{ header.robots }}">
        {% endif %}

        {% if page.taxonomy.tag %}
          <meta name="keywords" content="{% for tag in page.taxonomy.tag %}{{tag}},{% endfor %}">
        {% else %}
          <meta name="keywords" content="{{ site.metadata.keywords }}">
        {% endif %}

        <link rel="icon" type="image/png" href="{{ theme_url }}/images/favicon.png">
        <link rel="shortcut icon" href="{{ theme_url }}/images/favicon.ico">

        <title>{% if header.title %}{{ header.title }} | {% endif %}{{ site.title }}</title>


        <script>
          (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
          })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

          ga('create', 'UA-58825605-4', 'auto');
          ga('send', 'pageview');

        </script>


        {% block stylesheets %}
            {# Bootstrap core CSS #}
            {% do assets.add('theme://css/bootstrap.min.css',101) %}
            {% do assets.add('theme://css/carousel.css',101) %}
            {% do assets.add('https://fonts.googleapis.com/css?family=Open+Sans:400,600,700,300',101) %}
            {% do assets.add('https://fonts.googleapis.com/css?family=Work+Sans:800,900',100) %}

            {# Custom styles for this theme #}
            {% do assets.add('theme://css/bootstrap-custom.css',100) %}
            {% do assets.add('theme://css/article-listing.css',100) %}
            {% do assets.add('theme://css/header.css',100) %}
            {% do assets.add('theme://css/navbar.css',100) %}
            {% do assets.add('theme://css/review.css',100) %}
            {% do assets.add('theme://css/articles-3-columns-alternative.css',100) %}
            {% do assets.add('theme://css/articles-3-columns.css',100) %}
            {% do assets.add('theme://css/slick.css',100) %}
            {% do assets.add('theme://css/articles.css',100) %}
            {% do assets.add('theme://css/simplesearch_results.css',100) %}
            {% do assets.add('theme://css/categories.css',100) %}

            {{ assets.css() }}
        {% endblock %}

        {% block javascripts %}
            {% do assets.add('https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js', 101) %}
            {% do assets.add('theme://js/bootstrap.min.js') %}
            {% do assets.add('theme://js/slick.min.js') %}
            {% do assets.add('theme://js/articles-3-columns-alternative.js') %}
            {% do assets.add('theme://js/articles-3-columns.js') %}
            {% do assets.add('https://use.fontawesome.com/d409eefb1e.js') %}

            {% if browser.getBrowser == 'msie' and browser.getVersion >= 8 and browser.getVersion <= 9 %}
                {% do assets.add('https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js') %}
                {% do assets.add('https://oss.maxcdn.com/respond/1.4.2/respond.min.js') %}
            {% endif %}

            {% do assets.add('theme://js/parallax.min.js') %}
            {% do assets.add('theme://js/navbar.js') %}

            {{ assets.js() }}
        {% endblock %}

    {% endblock head %}
</head>

<body>

<div class="container header_container">
    {% include 'partials/header.html.twig' %}
</div>

{% include 'partials/navbar.html.twig' %}

{% block content %}{% endblock %}

{% include 'partials/footer.html.twig' %}

</body>
{% block bottom %}{% endblock %}
</html>
